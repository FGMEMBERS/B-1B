<?xml version="1.0"?>
<PropertyList>

 <path>rain_moving.osg</path>
 <offsets>
    <x-m> 0.0 </x-m>
    <y-m> 0.0 </y-m>
    <z-m> -6.0 </z-m>
    
    <pitch-deg>0</pitch-deg>
 </offsets>

 <animation>
	 <type>rotate</type>
	 <path>rain_moving.osg</path>
	 <property>velocities/airspeed-kt</property>
	 <interpolation>
		 <entry>
			 <ind>0.0</ind>
			 <dep>0.0</dep>
		 </entry>
		 <entry>
			 <ind>220.0</ind>
			 <dep>-90.0</dep>
		 </entry>
	 </interpolation>
	 <center>
		 <x-m>0</x-m>
		 <y-m>0</y-m>
		 <z-m>14</z-m>
	 </center>
	 <axis>
		 <x>0</x>
		 <y>1</y>
		 <z>0</z>
	 </axis>
 </animation>
 
 
 
</PropertyList>
