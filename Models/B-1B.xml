<PropertyList>

  <path>b1b.ac</path>
  
  <offsets>
	  <x-m>25.0</x-m>
	  <y-m>0.0</y-m>
          <z-m>0.24</z-m>
    <pitch-deg>0</pitch-deg>
    
  </offsets>
  
  <!-- cockpit instruments -->
  
  <model>
	  <name>instruments</name>
	  <path>Aircraft/B-1B/Models/instruments.xml</path>
	  <offsets>
		  <offsets>
			  <x-m>25.0</x-m>
			  <y-m>0.0</y-m>
			  <z-m>0.24</z-m>
			  <pitch-deg>0</pitch-deg>
		  </offsets>
	  </offsets>
  </model>
  
  <!-- trans parts -->
  <model>
	  <name>transparent parts</name>
	  <path>Aircraft/B-1B/Models/b1b_trans.xml</path>
	  <offsets>
		  <offsets>
			  <x-m>25.0</x-m>
			  <y-m>0.0</y-m>
			  <z-m>0.24</z-m>
			  <pitch-deg>0</pitch-deg>
                  </offsets>
	  </offsets>
  </model>

  <!-- correct wing position -->
  <animation>
	  <type>translate</type>
	  <object-name>wing_right</object-name>
	  <object-name>slat_right</object-name>
	  <object-name>slat_right_up</object-name>
	  <object-name>flap_right1</object-name>
	  <object-name>flap_right2</object-name>
	  <object-name>flap_right3</object-name>
	  <object-name>flap_right1_up</object-name>
	  <object-name>flap_right2_up</object-name>
	 <object-name>flap_right3_up</object-name>
	 <object-name>spoiler_right1</object-name>
	 <object-name>spoiler_right2</object-name>
	 <object-name>spoiler_right3</object-name>
	 <object-name>spoiler_right4</object-name>
	  <property>rotors/main/blade[0]/position-deg</property>
	  
	  <interpolation>
		  <entry>
			  <ind>0.0</ind>
			  <dep>-0.6</dep>
		  </entry>
		  <entry>
			  <ind>1.0</ind>
			  <dep>-0.6</dep>
		  </entry>
	  </interpolation>
	  <axis>
		  <x>-1</x>
		  <y>0</y>
		  <z>0</z>
	  </axis>
	  
 </animation>
 
