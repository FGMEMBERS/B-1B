<!--open/close bay doors  -->
<animation>
	 <type>pick</type>
	 <object-name>sms_button.024</object-name>
	 <action>
		 <button>0</button>
		 <repeatable>true</repeatable>
		 <interval-sec>0.3</interval-sec>
		 <binding>
			      <command>nasal</command>
			      <script>b1b.bay_fwd.open()</script>
		 </binding>

	 </action>
</animation>
 <animation>
	 <type>textranslate</type>
	 <object-name>sms_button.024</object-name>
	 <property>doors/bay_fwd/position-norm</property>
	 <factor>0.038</factor>
         <step>1</step>
	 <axis>
		 <x>0.0</x>
		 <y>1.0</y>
		 <z>0.0</z>
	 </axis>
 </animation>
